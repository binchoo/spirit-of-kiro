{
  "name": "websocket-server",
  "version": "1.0.1",
  "description": "A simple Node.js websocket server",
  "main": "server.ts",
  "scripts": {
    "start": "bun server.ts",
    "dev": "bun --watch server.ts",
    "test": "bun test --bail --reporter=junit --reporter-outfile=./bun-tests.xml",
    "validate-config": "bun scripts/validate-config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.825.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "js-yaml": "^4.1.0",
    "redis": "^5.0.1",
    "sharp": "^0.34.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "yaml": "^2.3.4"
  },
  "aiEditorAgent": {
    "testConfig": {
      "script": "test",
      "testReportPath": "./bun-tests.xml"
    }
  }
}
